public class Swap {
	public int[] reverse(int[] v, int N) {
	    int i = 0;
	    int j = N - 1;
	    while (i < j) {
	    	
	        swap(v, i, j);  // this is a self-defined function
	        i++;
	        j--;
	    }
	System.out.println(v);
	 return v;
	    
	}


		public static void swap(int[] t , int a ,int b) {
			int tem ;
			tem = t[a];
			t[a] = t[b];
			t[b] = tem;
		}

		public static void main(String[] args) {
			Swap swap = new Swap();
			int[] w = {1,2,3,45,6,7};
			int[] m = {};
			m = swap.reverse(w, 6);
			System.out.println(m);
		}

}