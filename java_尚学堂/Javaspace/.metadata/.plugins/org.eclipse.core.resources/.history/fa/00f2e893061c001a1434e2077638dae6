import java.awt.*;
import javax.swing.*;


public class BallGame1 extends JFrame {

	void launchFrame() {
		setSize(300,300);
		setLocation(400,400);
		setVisible(true);
	}
	public static void main(String[] args) {
		System.out.println('程序的开始启动');
		BallGame1 game = new BallGame1();
		game.launchFrame();
	}
	
	
}
