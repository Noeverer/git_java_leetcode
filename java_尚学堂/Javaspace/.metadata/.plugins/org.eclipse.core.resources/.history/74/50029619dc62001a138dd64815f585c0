package p5;

import p5.extends_p5_1.Person.Student;

public class extends_p5_1 {
	public static void main(String[] args) {
		Student stu = new Student("liuxiangqian",175,"java");
		stu.study();
		stu.rest();
		
	}
	
	
	
	
	
	
	class Person{
		String name;
		int height;
		public void rest() {
			System.out.println("敲代码累了，休息一会！！！");
		}
	}
		
	class Student extends Person(String name,int height ,String major){
		String mojor;
		public void study(){
			System.out.println("努力学习挖掘机技术！！！");
		   }
	    }
	

}
